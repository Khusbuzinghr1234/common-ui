{
  "name": "common-ui",
  "version": "1.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "react-native": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "react-native": "./dist/index.mjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --splitting false",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.72"
  },

  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.0"
  }
}

{
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["react", "react-native", "tamagui"],
    "moduleResolution": "bundler",
    "moduleSuffixes": [".native", ".web", ""],
    "target": "ES2019",
    "module": "ESNext",
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src", "src/**/*.d.ts"]
}
